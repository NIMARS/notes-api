{
  "name": "notes-api",
  "version": "1.0.0",
  "description": "notes like it must be",
  "keywords": [
    "notes"
  ],
  "homepage": "https://github.com/NIMARS/notes-api#readme",
  "bugs": {
    "url": "https://github.com/NIMARS/notes-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NIMARS/notes-api.git"
  },
  "license": "ISC",
  "author": "NIMARS",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "test": "node --experimental-vm-modules --no-warnings=ExperimentalWarning ./node_modules/jest/bin/jest.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "migrate:dev": "prisma migrate dev --name init_notes",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "seed": "tsx prisma/seed.ts",
    "lint": "eslint . --ext .ts",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down -v"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-jest": "^29.0.1",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "pino": "^9.4.0",
    "typescript-eslint": "^8.45.0",
    "zod": "^4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.2"
  }
}
