{
  "name": "notes-api",
  "version": "1.0.0",
  "description": "notes like it must be",
  "keywords": [
    "notes"
  ],
  "homepage": "https://github.com/NIMARS/notes-api#readme",
  "bugs": {
    "url": "https://github.com/NIMARS/notes-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NIMARS/notes-api.git"
  },
  "license": "ISC",
  "author": "NIMARS",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "scripts": {
    "build": "npm run generate && tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev --name init_notes",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "seed:perf": "tsx prisma/seed.ts --count=50000 --batch=1000",
    "lint": "eslint . --ext .ts",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:contract": "vitest run --config vitest.integration.config.ts tests/contracts",
    "test:e2e": "vitest run --config vitest.integration.config.ts tests/e2e",
    "test:all": "npm run test:unit && npm run test:contract && npm run test:e2e",
    "test": "npm run test:all",
    "test:watch": "vitest --config vitest.unit.config.ts",
    "test:coverage": "vitest run --coverage",
    "bench:notes": "tsx scripts/bench-notes.ts",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down -v",
    "audit:prod": "npm audit --omit=dev"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "pg": "^8.18.0",
    "pino": "^9.4.0",
    "prisma": "^7.4.0",
    "zod": "^4.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "supertest": "^7.1.3",
    "tslib": "^2.7.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
